@model IEnumerable<ScientificResearch.ViewModel.ERPNWorkToDoViewModel>

                      <div id="example">
                          <div class="wrapper">
                              <div id="tabstrip">
                                  <ul>
                                      <li class="k-state-active">日常经费记录</li>
                                      <li>统计分析</li>
                                  </ul>
                              </div>
                          </div>
                      </div>

                      <script>
                          $(document).ready(function () {
                              var ts = $("#tabstrip").kendoTabStrip({
                                  animation: { open: { effects: "fadeIn" } },
                                  contentUrls: [
                                              '/Statistic/FundsList?moduleName=PaperPublish',
                                              '/Statistic/FundsManageProgramStatistics?formId=1052'
                                  ]
                              }).data('kendoTabStrip');
                          });
                      </script>

                      <style>
                          .wrapper {
                              height: 100%;
                              margin: 20px auto;
                              padding: 20px 0 0 0;
                              background: url('../content/web/tabstrip/bmw.png') no-repeat center 60px transparent;
                          }

                          #tabstrip {
                              max-width: 100%;
                              float: right;
                              margin-bottom: 20px;
                          }

                              #tabstrip .k-content {
                                  height: 100%;
                                  overflow: auto;
                              }

                          .specification {
                              max-width: 100%;
                              margin: 10px 0;
                              padding: 0;
                          }

                              .specification dt, dd {
                                  max-width: 100%;
                                  float: left;
                                  margin: 0;
                                  padding: 5px 0 8px 0;
                              }

                              .specification dt {
                                  clear: left;
                                  width: 100px;
                                  margin-right: 7px;
                                  padding-right: 0;
                                  opacity: 0.7;
                              }

                              .specification:after, .wrapper:after {
                                  content: ".";
                                  display: block;
                                  clear: both;
                                  height: 0;
                                  visibility: hidden;
                              }
                      </style>



    