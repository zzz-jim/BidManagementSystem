    <script type="text/javascript">
        function OpeToDoWindow(id) {
            $("#AllToDoWindow").kendoWindow({
                position: {
                    top: "15%", // or "100px"
                    left: "13%"
                },
                width: "80%",
                height: "80%",
                modal: true,
                draggable: true,
                title: "待办",
                visible: false,
                actions: [
                        "Pin",
                        "Close",
                       // "Minimize",
                     // "Maximize"
                ],
                resizable: false,
                close: function (e) {
                    parent.location.reload();//刷新页面
                }
            });

            $.ajax({
                url: "/Approval/RiZhiReturnStateValue?id=" + id,
                type: "post",
                success: function (data) {
                    if (data != null) {
                        // $("#MaxWindow").html(data);
                        var k = data;
                        //判断返回的状态来控制弹出框第一个打开的页面
                        //申请书已填写未审核、科室主任已经审批申请书、科教科已经审批申请书、科教科已经审批申请书、超级管理员审批已经审批申请书
                        if (data["ReturnState"] == "AplicationWriting") {
                            $("#AllToDoWindow").append(' <iframe id="application" width="100%" height="75%" scrolling="no" style="border:0px" src="/Approval/SubmitApplication?id=' + data["applicationId"] + '">项目申请书</iframe>');
                        }
                        else if (data["ReturnState"] == "ApplicationRejected" || data["ReturnState"] == "ApplicationApproving")//申请书已提交和待审批中
                        {
                            if (data["ReturnState"] == "ApplicationRejected") {
                                $("#AllToDoWindow").append(' <iframe id="application" width="100%" height="75%" scrolling="no" style="border:0px" src="/Approval/ApplicationRejected?id=' + data["applicationId"] + '">项目申请书</iframe>');
                            }
                            else {
                                $("#AllToDoWindow").append(' <iframe id="application" width="100%" height="75%" scrolling="no" style="border:0px" src="/Approval/ApplicationAgree?id=' + data["applicationId"] + '">项目申请书</iframe>');
                            }
                        }
                        else if (data["ReturnState"] == "ApplicationApproved" || data["ReturnState"] == "ProjectEstablishing")//全部通过审批
                        {
                            //为申请书iframe的src赋值id是ERPNWorkToDo的ID
                            $("#AllToDoWindow").append(' <iframe id="application" width="100%" height="75%" scrolling="no" style="border:0px" src="/Approval/ProjectEstablish?id=' + data["applicationId"] + '">项目申请书</iframe>');
                        }
                        else if (data["ReturnState"] == "ProjectEstablished" || data["ReturnState"] == "ContractSigning")//申请书已提交和待审批中
                        {
                            $("#AllToDoWindow").append(' <iframe id="process" width="100%" height="75%" scrolling="no" style="border:0px" src="/Approval/SignContract?id=' + data["applicationId"] + '">合同记录</iframe>');
                        }
                        else if (data["ReturnState"] == "ContractSigned" || data["ReturnState"] == "ProjectProcessing")//申请书已提交和待审批中
                        {
                            $("#AllToDoWindow").append(' <iframe id="process" width="100%" height="75%" scrolling="no" style="border:0px" src="/Approval/ProcessRecordList?id=' + data["applicationId"] + '">过程记录</iframe>');
                        }
                        else if (data["ReturnState"] == "ConcludeUnWrite" || data["ReturnState"] == "ConcludeUnSubmit")//申请书已提交和待审批中
                        {
                            $("#AllToDoWindow").append(' <iframe id="process" width="100%" height="75%" scrolling="no" style="border:0px" src="/Approval/Conclusions?id=' + data["applicationId"] + '">项目结案</iframe>');
                        }
                        else if (data["ReturnState"] == "ProjectConcluding" || data["ReturnState"] == "ProjectConcluded" || data["ReturnState"] == "ConcludeRejected")//申请书已提交和待审批中
                        {
                            if (data["ReturnState"] == "ConcludeRejected") {
                                $("#AllToDoWindow").append(' <iframe id="process" width="100%" height="75%" scrolling="no" style="border:0px" src="/Approval/ConclusionsRejected?id=' + data["applicationId"] + '">项目结案</iframe>');
                            }
                            else {
                                $("#AllToDoWindow").append(' <iframe id="process" width="100%" height="75%" scrolling="no" style="border:0px" src="/Approval/ConclusionsAgree?id=' + data["applicationId"] + '">项目结案</iframe>');
                            }
                        }
                        else if (data["ReturnState"] == "ExtensionUnWrite")//
                        {
                            //为申请书iframe的src赋值id是ERPNWorkToDo的ID
                            $("#AllToDoWindow").append(' <iframe id="process" width="100%" height="75%" scrolling="no" style="border:0px" src="/Approval/ExtensionRequest?id=' + data["applicationId"] + '">项目延期</iframe>');
                        }
                        else if (data["ReturnState"] == "ExtensionRequestApproving" || data["ReturnState"] == "ExtensionAgreed" || data["ReturnState"] == "ExtensionRejected")//申请书已提交和待审批中
                        {
                            if (data["ReturnState"] == "ExtensionRejected") {
                                $("#AllToDoWindow").append(' <iframe id="process" width="100%" height="75%" scrolling="no" src="/Approval/ExtensionRejected?id=' + data["applicationId"] + '">项目延期</iframe>');
                            }
                            else {
                                $("#AllToDoWindow").append(' <iframe id="process" width="100%" height="75%" scrolling="no" src="/Approval/ExtensionAgree?id=' + data["applicationId"] + '">项目延期</iframe>');
                            }
                        }

                    }
                    else {
                        alert("发生异常！");
                    }
                }
            });
            $("#AllToDoWindow").data("kendoWindow").center().open();
        };
    </script>
