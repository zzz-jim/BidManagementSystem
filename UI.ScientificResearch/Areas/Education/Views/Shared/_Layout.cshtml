                                for (var i = 0; i < 9; i++) {
                                    //将json时间转换
                                    var ChangingTimeStr = eval('new ' + data[i].TimeStr.replace('/', '', 'g').replace('/', '', 'g'));
                                    var ChangedTimeStr = ChangingTimeStr.toLocaleDateString() + " " + ChangingTimeStr.toLocaleTimeString();
                                    html = html + '<li class="tasksUl" style="border-bottom: 1px solid #F3F3F3;" onclick="OpeToDoWindow(' + data[i].FKApplicationID + ')"><a href="#"><div class="clearfix"><span class="pull-left">' + data[i].DoSomething + '</span> <span class="timeForNow pull-right">' + ChangedTimeStr + '</span></div></a></li>';
                                }
                            }
                            else {
                                for (var i = 0; i < data.length; i++) {
                                    //将json时间转换
                                    var ChangingTimeStr = eval('new ' + data[i].TimeStr.replace('/', '', 'g').replace('/', '', 'g'));
                                    var ChangedTimeStr = ChangingTimeStr.toLocaleDateString() + " " + ChangingTimeStr.toLocaleTimeString();
                                    html = html + '<li class="tasksUl" style="border-bottom: 1px solid #F3F3F3;" onclick="OpeToDoWindow(' + data[i].FKApplicationID + ')"><a href="#"><div class="clearfix"><span class="pull-left">' + data[i].DoSomething + '</span> <span class="timeForNow pull-right">' + ChangedTimeStr + '</span></div></a></li>';
                                }
