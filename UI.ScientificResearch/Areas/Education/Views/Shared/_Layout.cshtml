<!DOCTYPE html>
@using ScientificResearch.Utility.Constants;

@{
    var urlArray = Request.Url.Segments;
    if (urlArray.Length > 1)
    {
        if (urlArray[1] == "ContinuingEducation")
        {
            ViewBag.Module = "继教申请";
        }

        if (urlArray.Length > 2)
        {
            if (urlArray[2] == "Index")
            {
                ViewBag.Title = "简介";
            }
            else if (urlArray[2] == "WorkContainer")
            {
                ViewBag.Title = "待办事宜";
            }
            else if (urlArray[2] == "ApplicationList")
            {
                ViewBag.Title = "项目类学分";
            }
            else if (urlArray[2] == "NotProjectCreditList")
            {
                ViewBag.Title = "非项目类学分";
            }
            else if (urlArray[2] == "ProjectBonusCredit")
            {
                ViewBag.Title = "奖励设置";
            }
            else if (urlArray[2] == "ScienceFundsManageContainer")
            {
                ViewBag.Title = "资金统计";
            }
            else if (urlArray[2] == "CreditsStatistics")
            {
                ViewBag.Title = "学分统计";
            }
        }
    }
}
